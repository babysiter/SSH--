<?xml version="1.0"?>
 <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
 <hibernate-mapping> 
 	<class name="cn.edu.zjut.po.student_score" table="student_score" catalog="hibernatepro">
 		 <id name="studentID" type="int"> 
			 <column name="studentID" /> 
	 	 </id> 
	 	  <property name="studentAVG" type="java.lang.Double"> 
	 	 	<column name="studentAVG" /> 
	  	 </property>
	  	 <property name="studentSUM" type="java.lang.Double"> 
	 	 	<column name="studentSUM"/> 
	  	 </property>
	  	  <property name="studentName" type="string"> 
	 	 	<column name="studentName" length="45" /> 
	  	 </property>
	</class>
 </hibernate-mapping>